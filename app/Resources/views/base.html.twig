<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link href='{{ asset('bundles/app/css/template/font-awesome.css') }}' rel='stylesheet' type='text/css'>
        <link href='{{ asset('bundles/app/css/template/init.css') }}' rel='stylesheet' type='text/css'>
        <link href='{{ asset('bundles/app/css/template/style1.css') }}' rel='stylesheet' type='text/css'>
        <link href='{{ asset('bundles/app/css/template/animate.css') }}' rel='stylesheet' type='text/css'>
        {#<link href='{{ asset('bundles/app/css/template/responsive.css') }}' rel='stylesheet' type='text/css'>#}
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>

    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1"/>

</head>
<body class="template-index scheme_1">
<div class="page_wrapper">

    <div id="page_preloader__bg">
        <img id="page_preloader__img" src="https://cdn.shopify.com/s/files/1/0821/7433/t/3/assets/shopify_logo.gif?3"
             alt="">
    </div>

    <script>
        preloaderBg = document.getElementById('page_preloader__bg');
        preloaderImg = document.getElementById('page_preloader__img');

        window.addEventListener('load', function () {
            preloaderBg.classList.add("off");
            preloaderImg.classList.add("off");
        });

    </script>


    <div id="shopify-section-header" class="shopify-section">
        <header id="page_header">
            <div class="page_container">
                <div class="top_pannel inverted" style="background: #313131">
                    <div class="container">

                        <div class="top_pannel_menu">
                            <div class="header_account"></div>

                            <div class="header_wishlist"></div>
                            <div class="currency_selector">
                                <div class="currency_selector__wrap" title="Currency"></div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="header_wrap1 inverted" style="background: #373737">
                    <div class="container header_main_wrap">
                        <div class="header_wrap2 header_left">


                            <a class="header_logo" href="https://theme157-tools.myshopify.com">
                                <img src="//cdn.shopify.com/s/files/1/0821/7433/files/logo_270x161_crop_center@2x.png?v=1548158698" alt="Tools">

                            </a>



                        </div>
                        <div class="header_wrap2 header_center">


                            <div class="header_search">

                                <form action="/search" method="get" class="search-form" role="search">
                                    <input id="search-field" name="q" type="text" placeholder=" " class="hint">
                                    <button id="search-submit" type="submit"><i class="fa fa-search"></i></button>
                                </form>




                                <div id="search_result_container"></div>

                            </div>

                        </div>

                        <div class="header_wrap2 header_right">

                            <div class="header_cart" id="main__cart_item">

                                <a class="cart_link" href="/cart"><i class="fa fa-shopping-cart" aria-hidden="true"></i><span id="cart_items"><span class="count">0</span></span><span>item(s)</span></a>
                                <div class="checkout"><a href="/cart">Check out</a></div>



                            </div>

                        </div>
                    </div>
                </div>

                <div id="pseudo_sticky_block" style="height: 0px;"></div>

                <div id="bottom_pannel" class="header_wrap_bottom inverted" style="background: #232323">
                    <div class="container header_wrap3">

                        <nav id="megamenu" class="megamenu megamenu_desktop">




                            <ul id="megamenu_level__1" class="level_1 sf-js-enabled sf-arrows" style="">

                                <li id="item_1" class="level_1__item level_2__links">
                                    <a class="level_1__link" href="/collections">Collections
                                        {#<i class="level_1__trigger megamenu_trigger" data-submenu="menu_1548091585416"></i>#}
                                    </a>
                                </li>

                                <li id="item_2" class="level_1__item level_2__small">
                                    <a class="level_1__link active " href="https://theme157-tools.myshopify.com">
                                        Home
                                        {#<i class="level_1__trigger megamenu_trigger" data-submenu="menu_1548079106118"></i>#}
                                    </a>

                                    <ul class="level_2" id="menu_1548079106118" style="">
                                        <li>
                                            <div>
                                                <ul class="level_3">
                                                    <li class="level_3__item ">
                                                        <a class="level_3__link" href="/">Home</a>


                                                    </li>

                                                    <li class="level_3__item ">
                                                        <a class="level_3__link" href="/collections/all">Catalog</a>

                                                    </li>


                                                </ul>
                                            </div>
                                        </li>
                                    </ul>
                                </li>
                                <li id="item_3" class="level_1__item level_2__links">
                                    <a class="level_1__link " href="/collections/all">Catalog
                                        {#<i class="level_1__trigger megamenu_trigger" data-submenu="menu_1548091123017"></i><span class="menu_badge_top">hot deals</span>#}
                                    </a>
                                </li>



                            </ul>
                        </nav>

                    </div>
                </div>
            </div>
        </header>




    </div>
    {% block body %}

        {% block main %} {% endblock %}

    {% endblock %}

    <div id="shopify-section-footer" class="shopify-section">


        <footer style="background: #373737;">
            <div class="footer_row__1">
                <div class="container">

                    <div class="row footer_main">

                        <div class="col-sm-3 footer_block footer_block__2_1">

                            <div class="footer_item footer_item__about ">
                                <h3>About our company</h3>


                                {#<div class="about_text"><p>The theme is the great choice to launched your e-commerce#}
                                        {#project. You can choose it not only for fashion business, it's also fitting#}
                                        {#for electronics, car, cosmetics, furniture and other e-stores. The main#}
                                        {#Zemez Template will be supporting and updating to all new technical#}
                                        {#requirements.</p></div>#}

                            </div>

                        </div>

                        <div class="col-sm-3 footer_block "></div>





                        <div class="col-sm-3 footer_block footer_block__2_1"> </div>






                        <div class="col-sm-3 footer_block ">
                            <div class="footer_item footer_item__links ">
                                <h3>Collections</h3>
                                <ul>
                                    <li>
                                        <a href="/collections/abrasives">Abrasives</a>
                                    </li>

                                    <li>
                                        <a href="/collections/electrical">Electrical</a>
                                    </li>
                                </ul>
                            </div>


                        </div>


                    </div>

                </div>

            </div>


            <div class="footer_row__2 " style="background: #313131;">
                <div class="container">
                    <div class="footer_wrap_1">


                        <div class="footer_wrap_2 footer_center">
                            Â©Tools. <a target="_blank" rel="nofollow"
                                       href="https://www.shopify.com?utm_campaign=poweredby&amp;utm_medium=shopify&amp;utm_source=onlinestore">Powered
                                by Shopify</a>
                        </div>


                    </div>
                </div>
            </div>

        </footer>


        <style>
            #shopify-section-footer footer {
                padding-top: 0px;
            }
        </style>


    </div>

</div>

<a id="back_top" href="#">
    <i class="fa fa-angle-up" aria-hidden="true"></i>
</a>

</div>



{% block javascripts %}
    <script src="{{ asset('bundles/app/js/template/currencies.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/app/js/template/currency-tools.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/app/js/template/assets.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/app/js/template/shop.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/app/js/template/animate.js') }}" type="text/javascript"></script>
    {#<script  defer="defer" src="{{ asset('bundles/app/js/template/express_buttons.js') }}" type="text/javascript" crossorigin="anonymous"></script>#}
    {#<script  defer="defer" src="{{ asset('bundles/app/js/template/features.js') }}" type="text/javascript" crossorigin="anonymous"></script>#}
    {#<script  defer="defer" src="{{ asset('bundles/app/js/template/scripts.js') }}" type="text/javascript" crossorigin="anonymous"></script>#}






    {#<script defer="defer"#}
            {#src="https://cdn.shopify.com/s/assets/storefront/express_buttons-646307822f46ed653e67b5a23f6b2e4a7f38cb3a0df37b067fd9cc5898551c0c.js"#}
            {#crossorigin="anonymous"></script>#}
    {#<script defer="defer"#}
            {#src="https://cdn.shopify.com/s/assets/storefront/features-e87392afe29fe307282f4e6aad12cb4bcc2afefd6b7febf0b70edfd315f9684c.js"#}
            {#crossorigin="anonymous"></script>#}
    <script id="sections-script"
            data-sections="index-slideshow-full-width,index-products-carousel,index-image-with-text-overlay,index-newsletter,header,footer"
            defer="defer" src="https://cdn.shopify.com/s/files/1/0821/7433/t/3/compiled_assets/scripts.js?3"></script>

{% endblock %}
</body>

</html>
